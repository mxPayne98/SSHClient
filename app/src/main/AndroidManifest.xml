<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paynerealm.w3sshclient">

    <uses-feature
        android:name="android.hardware.wifi"
        android:required="true" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.paynerealm.w3sshclient.old.ActivityLogin"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">

        </activity>
        <activity
            android:name="com.paynerealm.w3sshclient.old.MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name="com.paynerealm.w3sshclient.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.paynerealm.w3sshclient.AddhostActivity" />
        <activity
            android:name="com.paynerealm.w3sshclient.TerminalActivity"
            android:label="TerminalActivity"
            android:windowSoftInputMode="adjustResize" />
    </application>


</manifest>